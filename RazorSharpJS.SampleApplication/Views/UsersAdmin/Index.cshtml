@model IEnumerable<ApplicationUser>

<h2>Index</h2>

<p>
    @Html.RazorSharpActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.RazorSharpActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.RazorSharpActionLink("Details", "Details", new { id = item.Id }) |
                @Html.RazorSharpActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
